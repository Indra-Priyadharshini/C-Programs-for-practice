int main() {
   int n1,n2,gcd,lcm,i,m;
   scanf("%d %d",&n1,&n2);
   if(n1>n2)
        m=n2;
    else
        m=n1;
    for(i=m;i>=1;i--)
    {
        if(n1%i==0&&n2%i==0)
        {
            gcd=i;
            break;//exiting from loop
        }
    }
    lcm=(n1*n2)/gcd;
    printf("\nLCM=%d",lcm);
    printf("\nGCD=%d",gcd);
    return 0;
}
