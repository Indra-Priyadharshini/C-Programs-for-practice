#include<ctype.h>
int main()
{
    char a[20];
    int i,j;
    fgets(a,20,stdin);
    for(i=0;a[i]!='\0';i++)
    {
        if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u'||a[i]=='A'||a[i]=='E'||a[i]=='I'||a[i]=='O'||a[i]=='U')
        {
            for(j=i;a[j]!='\0';j++)
            {
                a[j]=a[j+1];
            }
            a[j-1]='\0';
        }
    }
    printf("%s",a);
}

