#include <stdio.h>
#include<ctype.h>
int main(void) {
	// your code goes here
	char a[20];
	int i,l=0,w=0,c=0,s=0,t;
    gets(a);
	for(i=0;a[i]!='\0';i++)
	{
	    printf("%c",toupper(a[i]));
	    if(isdigit(a[i]))
	    {
	    l++;
	    t=a[i];
	    }
	    else if((a[i]>=65 && a[i]<=90)||(a[i])>=97&&a[i]<=122)
	    c++;
	    else
	    s++;
	    if(a[i]==' '&&a[i+1]!='\0')
	    w++;
	}
	printf("\nLength of string is %d",i);
	printf("\nNo. of Digits in string is %d",l); 
	printf("\nNo. of words in string is %d",w+1);
	printf("\nNo. of characters in string is %d",c);
	printf("\nNo. of special characters in string is %d",s);
	printf("\nt=%d",t);
	return 0;
}
