#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main()
{
    char s[5][100];
    for (int i=0; i<5; i++) fgets(s[i], 100, stdin);
    
    for (int i=0; i<4; i++)
    {
        for (int j=0; j<4-i; j++)
        {
            if (strlen(s[j])>strlen(s[j+1]))
            {
                char temp[100];
                strcpy(temp, s[j]);
                strcpy(s[j], s[j+1]);
                strcpy(s[j+1], temp);
            }
        }
    }
    
    for (int i=0; i<5; i++)
    {
        for (int j=0; j<strlen(s[i]); j++)
        {
            printf("%c", s[i][j]);
        }
    }
    
    return 0;
}
