#include<stdio.h>
//int* countFreq(char*);
char* removeMask(char*,char*);
int main()
{
    char s1[100],s2[100];
    fgets(s1,100,stdin);
    fgets(s2,100,stdin);
    printf("%s",removeMask(s1,s2));
    return 0;
}
char* removeMask(char* str1,char* str2)
{
   int count[256]={0};
   int i=0,j=0;
    for( i=0;*(str2+i)!='\0';i++)
    {
        count[str2[i]]++;
    }
  
    i=0;
    while(*(str1+i))
    {
        char x=*(str1+i);
        if(count[x]==0)
        {
            *(str1+j)=*(str1+i);
            j++;
        }
        i++;
    }
    *(str1+j)='\0';
    return str1;
}
