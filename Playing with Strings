#include <stdio.h>
#include <string.h>
#include <ctype.h>

int main()
{
    char s[100];
    int size;
    fgets(s, 100, stdin);
    size=strlen(s);
    
    for (int i=0; i<size-1; i++)
    {
        char a=tolower(s[i]), b=tolower(s[i+1]);
        if ((a=='a'||a=='e'||a=='i'||a=='o'||a=='u')&&(b!='a'&&b!='e'&&b!='i'&&b!='o'&&b!='u')) s[i+1]=toupper(s[i+1]);
    }
    
    for (int i=0; i<size; i++) printf("%c",s[i]);
    return 0;
}
