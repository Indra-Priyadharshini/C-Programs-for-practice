#include<stdio.h>
#include<ctype.h>
int main()
{
    char a[20];
    int i;
    fgets(a,20,stdin);
    for(i=0;a[i]!='\0';i++)
    {
        if(a[i]=='a'||a[i]=='e'||a[i]=='i'||a[i]=='o'||a[i]=='u'||a[i]=='A'||a[i]=='E'||a[i]=='I'||a[i]=='O'||a[i]=='U')
        {
            if(!(a[i+1]=='a'||a[i+1]=='e'||a[i+1]=='i'||a[i+1]=='o'||a[i+1]=='u'||a[i+1]=='A'||a[i+1]=='E'||a[i+1]=='I'||a[i+1]=='O'||a[i+1]=='U'))
            {
            a[i+1]=toupper(a[i+1]);
            }
        }
    }
    printf("%s",a);
}
